{
  "name": "vite-react-ts-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "pre": "vite build --mode pre",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,tsx,ts}' --quiet",
    "lint:fix": "eslint 'src/**/*.{js,jsx,tsx,ts}' --fix --quiet",
    "prettier": "prettier --write '**/*.{less,css,md,json}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "git add ."
    ],
    "*.{less,css,md,json}": [
      "npm run prettier",
      "git add ."
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "6.35.1",
    "antd": "^4.19.5",
    "antd-dayjs-vite-plugin": "^1.1.4",
    "axios": "^0.26.1",
    "dayjs": "^1.11.1",
    "less-vars-to-js": "^1.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "5.2.0",
    "yapi-to-ts": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-legacy": "^1.8.1",
    "@vitejs/plugin-react": "^1.3.0",
    "consola": "^2.15.3",
    "eslint": "^7.16.0",
    "eslint-config-ali": "^12.0.2",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "less": "^4.1.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-style-import": "^2.0.0"
  }
}
